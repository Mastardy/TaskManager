{
  "MongoDB": {
    "Host": "localhost:27017",
    "Cards": {
      "UserKey": "MONGO_USER",
      "PasswordKey": "MONGO_PASSWORD",
      "DatabaseKey": "MONGO_DB",
      "CollectionKey": "MONGO_COLLECTION"
    }
  },
  "Redis": {
    "Host": "localhost:6379"
  },
  "RabbitMQ": {
    "UserKey": "RABBITMQ_DEFAULT_USER",
    "PasswordKey": "RABBITMQ_DEFAULT_PASS"
  },
  "Authentication": {
    "GitHub": {
      "ClientIdKey": "GITHUB_CLIENT_ID",
      "ClientSecretKey": "GITHUB_CLIENT_SECRET"
    },
    "Jwt": {
      "SecretKey": "JWT_SECRET",
      "Issuer": "http://localhost:5254/",
      "Audience": "http://localhost:5254/",
      "ExpireMinutes": 60
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
